{
  "name": "mousenote",
  "version": "0.0.1",
  "description": "Mouse Note",
  "author": {
    "name": "AS",
    "email": "as@gmail.com"
  },
  "main": "dist/index.js",
  "scripts": {
    "ng": "ng",
    "build-electron": "tsc --project projects/electron/tsconfig.json",
    "package": "npm run build-electron && npm run build && electron-forge package",
    "make": "npm run build-electron && npm run build && electron-forge make",
    "publish-app": "npm run build-electron && npm run build && electron-forge publish",
    "start": "concurrently \"npm run build-electron\" \"ng serve MouseNote\" \"wait-on http://localhost:4200 && electron-forge start\"",
    "serve": "concurrently \"npm run build-electron\" \"ng serve MouseNote\" \"wait-on http://localhost:4200\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/cdk": "^18.1.0",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/material": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/platform-server": "^18.1.0",
    "@angular/router": "^18.1.0",
    "@angular/service-worker": "^18.1.0",
    "@angular/ssr": "^18.1.0",
    "@ng-bootstrap/ng-bootstrap": "^17.0.0",
    "@ngx-pwa/local-storage": "^18.0.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "material": "^0.9.14",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "update-electron-app": "^3.0.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.0",
    "@angular/cli": "^18.1.0",
    "@angular/compiler-cli": "^18.1.0",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
